{
  "common": {
    "name": "mysensors",
    "version": "0.2.0",
    "title": "mysensors adapter",
    "desc": {
      "en": "ioBroker mysensors Adapter",
      "de": "ioBroker Mysensors Adapter",
      "ru": "ioBroker Mysensors драйвер "
    },
    "platform":   "Javascript/Node.js",
    "mode":       "daemon",
    "icon":       "mysensors.png",
    "enabled":    true,
    "messagebox": true,
    "extIcon":    "https://raw.githubusercontent.com/ioBroker/ioBroker.mysensors/master/admin//mysensors.png",
    "keywords": [
      "mysensors",
      "arduino"
    ],
    "readme":     "https://github.com/ioBroker/ioBroker.mysensors/blob/master/README.md",
    "loglevel":   "info",
	"stopBeforeUpdate": true,
    "type":       "hardware"
  },
  "native": {
    "comName":      "",
    "connTimeout":  60000,
    "port":         5003,
    "baudRate":     115200,
    "mode":         "server",
    "type":         "udp",
    "bind":         "0.0.0.0"
  },
  "objects": [
  ],
  "instanceObjects": [
    {
      "_id":  "info",
      "type": "channel",
      "common": {
        "name": "Information"
      },
      "native": {}
    },
    {
      "_id":  "info.connection",
      "type": "state",
      "common": {
        "role":  "indicator.connected",
        "name":  "If connected to my sensors",
        "type":  "boolean",
        "read":  true,
        "write": false,
        "def":   false
      },
      "native": {}
    }
  ]
}
